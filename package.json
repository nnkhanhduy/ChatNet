    "buffer": "^6.0.3",
    "crypto-js": "^4.2.0",
    "elliptic": "^6.6.1",
    "hash.js": "^1.1.7",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-fs": "^2.20.0",
    "react-native-image-picker": "^5.7.0",
    "react-native-nitro-modules": "^0.31.10",
